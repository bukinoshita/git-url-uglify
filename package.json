{
  "name": "git-url-uglify",
  "version": "0.0.1",
  "description": "Uglify git url",
  "main": "index.js",
  "repository": "git@github.com:bukinoshita/git-url-uglify.git",
  "author": "Bu Kinoshita <bukinoshita@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "xo && ava"
  },
  "keywords": [
    "git",
    "github",
    "url",
    "uglify",
    "ugly",
    "ssh",
    "https",
    "repository",
    "repo"
  ],
  "xo": {
    "space": true,
    "semicolon": false,
    "esnext": true,
    "rules": {
      "object-curly-spacing": 0,
      "brace-style": 0,
      "max-statements-per-line": 0
    }
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "xo": "^0.18.2"
  }
}
